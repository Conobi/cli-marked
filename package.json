{
  "name": "cli-marked",
  "version": "4.9.7",
  "description": "A custom render for marked to output to the Terminal",
  "main": "index.js",
  "bin": {
    "md": "./cli.js"
  },
  "scripts": {
    "test": "FORCE_HYPERLINK=0 mocha tests/*.js --reporter spec",
    "fix": "eslint --quiet --fix *.js lib/*.js lib/*/*.js tests/*js .eslintrc.js --ignore-pattern '!.eslintrc.js'"
  },
  "files": [
    "index.js",
    "lib/",
    "cli.js"
  ],
  "keywords": [
    "marked",
    "render",
    "terminal",
    "markdown",
    "markdown-to-terminal",
    "cli",
    "cli-marked",
    "marked-cli"
  ],
  "author": "Grigorii Horos",
  "license": "MIT",
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "ansi-escapes": "^4.2.0",
    "cli-highlight": "^2.1.1",
    "cli-table3": "^0.5.1",
    "compose-function": "^3.0.3",
    "fieldset": "github:horosgrisa/fieldset",
    "figures": "^3.0.0",
    "languages-aliases": "^2.0.4",
    "node-emoji": "^1.10.0",
    "normalize-html-whitespace": "^1.0.0",
    "strip-ansi": "^5.2.0",
    "supports-hyperlinks": "^2.0.0",
    "wrap-ansi": "^6.0.0",
    "marked": "^0.7.0"
  },
  "directories": {
    "example": "example"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-standard-jsdoc": "^9.2.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^15.5.5",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^4.0.0",
    "eslint-plugin-unicorn": "^9.1.1",
    "mocha": "^6.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/horosgrisa/cli-marked.git"
  },
  "bugs": {
    "url": "https://github.com/horosgrisa/cli-marked/issues"
  },
  "homepage": "https://github.com/horosgrisa/cli-marked"
}
